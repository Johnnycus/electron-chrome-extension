{
  "name": "electron-chrome-extension",
  "version": "2.0.0-rc.3",
  "description": "Install Chrome extension in Electron",
  "main": "index.js",
  "private": true,
  "scripts": {
    "pretest": "tsc -p ./test",
    "test": "NODE_ENV=test concurrently \"npm:test-*\"",
    "test-watch": "tsc --watch -p ./test --outDir lib-test",
    "test-runner": "test/run.sh",
    "dl-extensions": "node playground/dl-extensions.js",
    "playground": "electron playground/main.js",
    "playground-cleandata": "rm -rf ~/Library/Application\\ Support/Electron/",
    "watch": "tsc --watch -p ./ --outDir lib",
    "lint": "tslint -p .",
    "build": "tsc -p ./ --outDir lib",
    "publish": " NPMInternalBucket=dev.getstation.com ./node_modules/.bin/npm-internal publish -f"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.5",
    "concurrently": "^4.1.0",
    "download-crx": "^1.0.5",
    "electron": "^3.0.4",
    "electron-local-crash-reporter": "^1.0.2",
    "electron-mocha": "^6.0.4",
    "electron-process-manager": "^0.6.0",
    "mocha": "^5.2.0",
    "tmp": "^0.0.33",
    "ts-mocha": "^2.0.0",
    "tslint": "^5.11.0",
    "tslint-config-station": "^0.5.1",
    "typescript": "^3.1.6",
    "unzip-crx": "^0.2.0",
    "xvfb-maybe": "^0.2.1"
  },
  "dependencies": {
    "capitalize": "^2.0.0",
    "electron-ipc-plus": "^1.3.4",
    "electron-simple-rpc": "github:alexstrat/electron-simple-rpc",
    "fs-extra": "^7.0.0",
    "npm-internal": "^1.4.0",
    "p-queue": "^3.0.0",
    "uuid": "^3.2.1"
  }
}
